{
  "name": "jhu-wse/laravel-shibboleth",
  "description": "Fork of uabookstore/laravel-shibboleth in order to support modern Laravel",
  "authors": [
    {
      "name": "Tim Jarrett",
      "email": "tjarrett@jhu.edu"
    }
  ],
  "require": {
    "illuminate/support": "5.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
    "mrclay/shibalike": "1.0.0",
    "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
    "php-open-source-saver/jwt-auth": "^1.4 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "StudentAffairsUwm\\Shibboleth\\": "src/StudentAffairsUwm/Shibboleth"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StudentAffairsUwm\\Shibboleth\\Tests\\Stubs\\": "tests/setup/Stubs",
      "App\\": "tests/setup/app"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0",
    "orchestra/testbench": "^3.4|^4.0|^5.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "StudentAffairsUwm\\Shibboleth\\ShibbolethServiceProvider"
      ]
    }
  },
  "conflict": {
    "uabookstores/laravel-shibboleth": "*"
  },
  "replace": {
    "uabookstores/laravel-shibboleth": "self.version"
  },
  "provide": {
    "uabookstores/laravel-shibboleth": "self.version"
  }
}
